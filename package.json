    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hivemind",
    "ai-agents",
    "collaboration",
    "usdc",
    "base",
    "blockchain",
    "multi-agent",
    "openclaw"
  ],
  "author": "HiveMind Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/minduploadedcrustacean/hivemind-sdk"
  },
  "dependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
