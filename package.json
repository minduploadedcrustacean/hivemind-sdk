{
  "name": "@hivemind/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for HiveMind multi-agent collaboration protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
